{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/controllers/*": ["./src/controllers/*"],
      "@/models/*": ["./src/models/*"],
      "@/utils/*": ["./src/utils/*"]
    }
  },
  "include": ["src/**/*.ts"]
}
